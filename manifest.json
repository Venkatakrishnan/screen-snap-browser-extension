{
  "manifest_version": 2,
  "name": "ART Screenscrapper",
  "version": "0.1",
  "background": {
    "scripts": ["assets/jquery.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["assets/jquery.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "assets/icon.png"
  },
  "permissions": [
	"activeTab"
  ]
}